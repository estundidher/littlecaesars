<% content_for :head do %>
  <meta name="author" content="ResponsiveWebInc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
	<%= favicon_link_tag 'apple-icon-180x180.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '180x180' %>
	<%= favicon_link_tag 'apple-icon-180x180.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '152x152' %>
	<%= favicon_link_tag 'apple-icon-144x144.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144' %>
	<%= favicon_link_tag 'apple-icon-120x120.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '120x120' %>
	<%= favicon_link_tag 'apple-icon-114x114.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '114x114' %>
	<%= favicon_link_tag 'apple-icon-76x76.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '76x76' %>
	<%= favicon_link_tag 'apple-icon-72x72.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '72x72' %>
	<%= favicon_link_tag 'apple-icon-60x60.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '60x60' %>
	<%= favicon_link_tag 'apple-icon-57x57.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '57x57' %>
	<%= favicon_link_tag 'apple-icon-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png' %>
	
	<%= favicon_link_tag 'android-icon-192x192.png', :rel => 'icon', :type => 'image/png', :sizes => '192x192' %>
	<%= favicon_link_tag 'android-icon-144x144.png', :rel => 'icon', :type => 'image/png', :sizes => '144x144' %>
	<%= favicon_link_tag 'android-icon-96x96.png', :rel => 'icon', :type => 'image/png', :sizes => '96x96' %>
	<%= favicon_link_tag 'android-icon-72x72.png', :rel => 'icon', :type => 'image/png', :sizes => '72x72' %>
	<%= favicon_link_tag 'android-icon-48x48.png', :rel => 'icon', :type => 'image/png', :sizes => '48x48' %>
	<%= favicon_link_tag 'android-icon-36x36.png', :rel => 'icon', :type => 'image/png', :sizes => '36x36' %>
	
	<!-- For all other devices -->
    <!-- Size should be 32 x 32 pixels -->
	<%= favicon_link_tag 'favicon.ico', :rel => 'shortcut icon' %>

	<%= favicon_link_tag 'manifest.json', :rel => 'manifest' %>

	<meta name="msapplication-TileColor" content="#ffffff">
	<meta name="theme-color" content="#ffffff">
<% end %>

<% content_for :content do %>

    <!-- Page Wrapper -->
    <div class="wrapper">

      <%= render partial: 'layouts/header' %>

      <%= content_for?(:content_top) ? yield(:content_top) : yield %>

      <% content_for :footer do %>
        <%= render partial:'layouts/footer' %>
      <% end %>

    </div><!-- / Wrapper End -->

    <!-- Scroll to top -->
    <span class="totop"><a href="#"><i class="fa fa-angle-up"></i></a></span>
<% end %>

<%= render template: "layouts/base" %>