<li class="dropdown <%= is_live ? 'active' : '' %>">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
      Live
    </a>
    <ul class="dropdown-menu multi-level" role="menu" aria-labelledby="dropdownMenu">
    	<li class="dropdown-submenu">
			<a tabindex="-1" href="#">Kitchen</a>
            <ul class="dropdown-menu">
        		<li> <%= link_to 'All', admin_order_live_path(nil, "kitchen"), :onclick => '$("body").addClass("loading");' %> </li>
              	<% Place.where(enabled:true).each do |place| %>
                	<li> <%= link_to place.name, admin_order_live_path(place, "kitchen"), :onclick => '$("body").addClass("loading");' %> </li>
              	<% end %>
            </ul>
		</li>
		<li class="divider"></li>
    	<li class="dropdown-submenu">
			<a tabindex="-1" href="#">Wrap-up</a>
            <ul class="dropdown-menu">
        		<li> <%= link_to 'All', admin_order_live_path(nil, "wrapup"), :onclick => '$("body").addClass("loading");' %> </li>
              	<% Place.where(enabled:true).each do |place| %>
                	<li> <%= link_to place.name, admin_order_live_path(place, "wrapup"), :onclick => '$("body").addClass("loading");' %> </li>
              	<% end %>
            </ul>
		</li>
		<li class="divider"></li>
		<li>
			<%= link_to 'Customer view', admin_order_live_customer_path, :onclick => '$("body").addClass("loading");' %>
		</li>            
    </ul>
</li>